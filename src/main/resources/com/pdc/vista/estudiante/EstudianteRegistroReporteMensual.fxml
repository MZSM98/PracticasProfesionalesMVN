      <ComboBox fx:id="comboReporteMensual" layoutX="142.0" layoutY="61.0" prefHeight="25.0" prefWidth="317.0" promptText="Seleccione reporte" />
      <Label layoutX="23.0" layoutY="65.0" text="Reporte mensual:" />
      <GridPane layoutX="24.0" layoutY="110.0" prefHeight="88.0" prefWidth="456.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
